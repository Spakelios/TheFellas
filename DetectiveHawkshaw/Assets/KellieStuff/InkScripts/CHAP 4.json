"str","^EmilUpset","/str",{"x()":"MC","exArgs":1},"out","/ev","\n","^\"... Has there?\"","\n","ev","str","^greys","/str",{"x()":"MCS","exArgs":1},"out","/ev","\n","ev","str","^transparent","/str",{"x()":"Evi","exArgs":1},"out","/ev","\n","ev","str","^Irritated","/str",{"x()":"Icon","exArgs":1},"out","/ev","\n","ev","str","^Svetlana","/str",{"x()":"Name","exArgs":1},"out","/ev","\n","ev","str","^EmilNeutral","/str",{"x()":"MC","exArgs":1},"out","/ev","^<I> I shake my head as we chat like friends again.","\n","^<I> It's a wonderfully nostalgic feeling.</I>","\n","ev","str","^Smiling","/str",{"x()":"Icon","exArgs":1},"out","/ev","^ \"Good God, no! Every other word is about looking forward to 'working with Dad again.'","\n","^\"I could never forgive myself if I got in the way of that...\"","\n","^\"Now, let's get to business.\"","\n","ev","str","^Confident","/str",{"x()":"Icon","exArgs":1},"out","/ev","\n","^\"...Tell me about this file of yours then.\"","\n","end",{"#f":1}],"global decl":["ev",false,{"VAR=":"canShowEvidence"},0,{"VAR=":"someEvidence"},0,{"VAR=":"count"},0,{"VAR=":"ShowJournal"},"/ev","end",null],"#f":1}],"listDefs":{}}